{
    "manifestVersion": 1,
    "id": "merge-a-bot",
    "publisher": "alex-h",
    "version": "1.0.0",
    "name": "merge-a-bot",
    "description": "Handles merging branches automatically based on defined policies like source -> target branch or release branches -> master",
    "public": false,
    "categories": ["Azure Repos"],
    "tags": ["code", "repo", "git", "branch", "merge"],
    "screenshots": [
        {
            "path": "images/blank.png"
        },
        {
            "path": "images/create.png"
        },
        {
            "path": "images/table.png"
        }
    ],
    "demands": [
        "api-version/5.1"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "icons": {
        "default": "images/icon-128-black.png"
    },
    "content": {
        "details": {
          "path": "overview.md"
        },
        "license": {
            "path": "license.md"
        }
    },
    "links": {
        "home": { "uri": "https://github.com/epignosisx/azure-devops-merge-bot" },
        "learn": { "uri": "https://github.com/epignosisx/azure-devops-merge-bot" },
        "support": { "uri": "https://github.com/epignosisx/azure-devops-merge-bot/issues" },
        "privacypolicy": { "uri": "https://creativecommons.org/privacy/" }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/epignosisx/azure-devops-merge-bot"
    },
    "contributions": [
        {
            "id": "merge-policies-hub",
            "type": "ms.vss-web.hub",
            "targets": [
                "ms.vss-code-web.code-hub-group"
            ],
            "properties": {
                "name": "Merge Policies",
                "uri": "merge-policies-hub.html",
                "icon": {
                    "light": "images/icon-128-black.png",
                    "dark": "images/icon-128-white.png"
                }
            }
        }
    ],
    "files": [
        {
            "path": "merge-policies-hub.html",
            "addressable": true
        },
        {
            "path": "dist",
            "addressable": true,
            "packagePath": "dist"
        },
        {
            "path": "images",
            "addressable": true,
            "packagePath": "images"
        }
    ]
}